{
    "compilerOptions": {
        "target": "es2016",
        "module": "commonjs",
        "outDir": "./dist",
        "rootDir": "./src",
        "esModuleInterop": true,
        "forceConsistentCasingInFileNames": true,
        "typeRoots": ["./types"],
        "strict": true,
        "skipLibCheck": true,
        "sourceMap": true,
        "declaration": true
    }
}
